<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title>HomePage</title>
  <meta name="viewport" content="width=device-width, initial-scale=1 ,user-scalable=no">
</head>

<body>
  <div id="right_wapper">
    <header id="header_wapper" class="clearfix">
      <%-include("/components/header/admin-header")%>
        <%-include("/components/nav/admin-topNav")%>
    </header>
    <div id="content_wapper" class="container-fluid">
      <%-include("/content/myorder-content")%>
    </div>
  </div>

  <link rel="stylesheet" href="./css/plugs/bootstrap.css">
  <link rel="stylesheet" href="./css/main.css">
  <link rel="stylesheet" href="./css/content.css">
  <link rel="stylesheet" href="./css/components.css">
  <link rel="stylesheet" href="./css/plugs/buttons.css">
  <link rel="stylesheet" href="./css/plugs/blue.css">
  <link rel="stylesheet" href="./css/plugs/font-awesome.min.css">
  <link rel="stylesheet" href="./css/plugs/datetimepicker.css">

  <script type="text/javascript" src="./js/plugs/jquery.js"></script>
  <script type="text/javascript" src="./js/plugs/bootstrap.js"></script>
  <script type="text/javascript" src="./js/plugs/bootstrap-datetimepicker.js"></script>
  <script type="text/javascript" src="./js/plugs/icheck.js"></script>
  <script type="text/javascript" src="./js/plugs/xss.js"></script>

  <script type="text/javascript" src="./js/init.js"></script>
  <script type="text/javascript" src="./js/components.js"></script>
  <script type="text/javascript" src="./js/content.js"></script>


  <script type="text/javascript">
    $(document).ready(function() {
      for (var i in init) {
        for (var j in init[i])
          init[i][j]()
      }
    })

    var del = function(rid) {
      $("#delete")
        .modal()
    }
    var edit = function(rid) {
      $("#purchaseRequisition")
        .modal()
    }
    //多模态HACK
    var couter = 0;
    $(document).on('hidden.bs.modal', '.modal', function(e) {
      $(this).css("overflow-x", "auto")
      $(this).css("overflow-y", "auto")
      $(this).css("z-index", 1050)
      couter--
    });
    $(document).on('show.bs.modal', '.modal', function(e) {
      $(this).css("z-index", 1050 + couter)
      couter++
    });
  </script>

</body>

</html>
