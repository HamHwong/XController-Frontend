<div class="modal fade" id="addAgent" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title" id="myModalLabel">添加代理商</h4>
      </div>

      <div class="modal-body">
        <form id="add_Agent" class="form-horizontal" role="form">
          <%-include('/components/input/forminput',{name:"姓名",id:"name",type:"text",placeholder:"请输入姓名",required:true})%>
            <%-include('/components/input/forminput',{name:"手机号",id:"phone",type:"text",placeholder:"请输入手机号",required:true})%>
              <%-include('/components/input/forminput',{name:"账号",id:"user",type:"text",placeholder:"请输入账号",required:true})%>
                <%-include('/components/input/forminput',{name:"密码",id:"password",type:"password",placeholder:"请输入密码",required:true})%>
                  <%-include('/components/input/forminput',{name:"邮箱",id:"email",type:"text",placeholder:"请输入邮箱",required:true})%>
        </form>
      </div>

      <div class="modal-footer">
        <button type="submit" class="btn btn-primary center-block col-xs-12">完成</button>
      </div>
    </div>
    <!-- /.modal-content -->
  </div>
  <!-- /.modal -->
</div>
