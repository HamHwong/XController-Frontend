<script type="text/javascript">
    /**
   * @description Table初始化
   */
  var table_init = function() {
    var $tablecontainer = $("<%=locals.container%>")
    var t = new table("<%=locals.url%>",<% if(locals.hasButton){%>{hasButton:<%=locals.hasButton%>}<%}%>)
    t.load().to($tablecontainer)
    t.bindModal()
  }

</script>
